<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>qrmarksspot.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>home</managed-bean-name>
        <managed-bean-class>qrmarksspot.home</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>qrmarksspot.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>qrmarksspot.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>qrmarksspot.login</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>signup</managed-bean-name>
        <managed-bean-class>qrmarksspot.signup</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/home.jsp</from-view-id>
        <navigation-case>
            <from-outcome>signup</from-outcome>
            <to-view-id>/signup.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createGame</from-outcome>
            <to-view-id>/createGame.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gamePage</from-outcome>
            <to-view-id>/gamePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/search.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failPlay</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/profilePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>abusementMessage</from-outcome>
            <to-view-id>/abusementMessage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>showQr</from-outcome>
            <to-view-id>/showQr.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case3</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/signup.jsp</from-view-id>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/signup.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>createGame</managed-bean-name>
        <managed-bean-class>qrmarksspot.createGame</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>gameSteps</managed-bean-name>
        <managed-bean-class>qrmarksspot.gameSteps</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/createGame.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/gameSteps.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/gameSteps.jsp</from-view-id>
        <navigation-case>
            <from-outcome>finished</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>gamePage</managed-bean-name>
        <managed-bean-class>qrmarksspot.gamePage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>playGame</managed-bean-name>
        <managed-bean-class>qrmarksspot.playGame</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>gameFinish</managed-bean-name>
        <managed-bean-class>qrmarksspot.gameFinish</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>success</managed-bean-name>
        <managed-bean-class>qrmarksspot.success</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/playGame.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/success.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>normal</from-outcome>
            <to-view-id>/playGame.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/gamePage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>playGame</from-outcome>
            <to-view-id>/playGame.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>signUp</from-outcome>
            <to-view-id>/signup.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>forum</managed-bean-name>
        <managed-bean-class>qrmarksspot.forum</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>search</managed-bean-name>
        <managed-bean-class>qrmarksspot.search</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>profilePage</managed-bean-name>
        <managed-bean-class>qrmarksspot.profilePage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/profilePage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>homepage</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>signup</from-outcome>
            <to-view-id>/signup.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/profilePage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/search.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/gamePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/gamePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case3</from-outcome>
            <to-view-id>/gamePage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>messageSent</managed-bean-name>
        <managed-bean-class>qrmarksspot.messageSent</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>abusementMessage</managed-bean-name>
        <managed-bean-class>qrmarksspot.abusementMessage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>showQr</managed-bean-name>
        <managed-bean-class>qrmarksspot.showQr</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/abusementMessage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>messageSent</from-outcome>
            <to-view-id>/messageSent.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
